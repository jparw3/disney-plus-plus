{
    "manifest_version": 2,
    "name": "Disney Plus Plus",
    "description": "A chrome extension that automatically plays episodes on Disney Plus",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  